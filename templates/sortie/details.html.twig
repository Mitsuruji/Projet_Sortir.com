<tr>
    <td>{{ sortie.nom }}</td>
    <td>{{ sortie.dateHeureDebut|date("d/m/y H:i") }}</td>
    <td>{{ sortie.dateLimiteInscription|date("Y-m-d") }}</td>
    <td>{{ sortie.participantInscrit.count }} / {{ sortie.nbInscriptionsMax }}</td>
    <td>{{ sortie.etat.libelle }}</td>
    <td>todo inscrit</td>
    <td>{{ sortie.participantOrganisateur.username }}</td>
    <td>
        {% if app.user.username==sortie.participantOrganisateur.username %}
            <a href="#" title="Afficher sortie">Modifier</a>
        {% else%}
            <a href="#" title="Afficher sortie">Afficher</a> -

            {% if inscrit == true %}
                  <a href="{{ path('#', {'idSortie':sortie.id, 'idParticipant':app.user.id}) }}" title="Se desister sortie">Se desister</a>
            {% else %}
                <a href="{{ path('sortie_inscription', {'idSortie':sortie.id, 'idParticipant':app.user.id}) }}" title="S'incrire sortie">S'inscrire</a>
            {% endif %}

        {% endif %}
    </td>
</tr>


